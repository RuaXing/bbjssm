			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>询问提示语</description>
			<name>tipMsg</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<!-- 指定支持动态属性 -->
